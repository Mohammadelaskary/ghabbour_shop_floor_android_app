<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <CheckBox
        android:id="@+id/defectname_checkBox"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="shaseh"
        android:padding="8dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/child"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/defectname_checkBox"
        app:layout_constraintTop_toTopOf="@id/defectname_checkBox"
        app:layout_constraintEnd_toStartOf="@+id/defectedqty"
        android:layout_marginTop="2dp"
        android:layout_marginStart="80dp"
        android:text="@string/childdesc_txt"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/defectedqty"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/child"
        app:layout_constraintEnd_toStartOf="@id/repairedqty"
        app:layout_constraintTop_toTopOf="@id/child"
        android:text="155"
        android:layout_marginStart="65dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/repairedqty"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/defectedqty"
        app:layout_constraintStart_toEndOf="@+id/defectedqty"
        app:layout_constraintEnd_toStartOf="@id/notes"
        android:text="120"
        android:layout_marginStart="45dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        />


    <TextView
        android:id="@+id/notes"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/repairedqty"
        app:layout_constraintStart_toEndOf="@+id/repairedqty"
        android:text="need to enchance welding"
        app:layout_constraintEnd_toStartOf="@id/status"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/status"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/notes"
        app:layout_constraintStart_toEndOf="@+id/notes"
        android:text="Ok"
        android:layout_marginStart="60dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        />


</androidx.constraintlayout.widget.ConstraintLayout>